{
  "name": "claude-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "clean": "rm -rf .next node_modules",
    "preinstall": "npx only-allow npm",
    "prepare": "husky install",
    "perf:audit": "lighthouse http://localhost:3000 --output json --output html --output-path ./performance",
    "perf:build-stats": "npx next build --debug",
    "perf:bundle-analyze": "npm run build && npx @next/bundle-analyzer",
    "benchmark": "node scripts/performance-benchmark.js",
    "vercel-build": "npm run build",
    "postinstall": "echo 'Installation complete'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "compression": "^1.7.4",
    "critters": "^0.0.25",
    "helmet": "^8.0.0",
    "immer": "^10.1.1",
    "jsonwebtoken": "^9.0.2",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "ws": "^8.18.0",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@next/bundle-analyzer": "^15.4.5",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/axe-core": "^3.0.6",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.14",
    "axe-core": "^4.10.3",
    "eslint-config-next": "15.4.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "tailwindcss": "^4",
    "typescript": "5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}